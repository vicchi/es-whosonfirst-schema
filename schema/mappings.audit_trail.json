{
    "mappings": {
        "logs": {
            "properties": {
                "@event": {
                    "type": "string"
                },
                "@id": {
                    "type": "string"
                },
                "@timestamp": {
                    "format": "dateOptionalTime",
                    "type": "date"
                },
                "@version": {
                    "type": "string"
                },
                "ok": {
                    "type": "integer"
                },
                "task": {
                    "type": "string"
                }
            }
        }
    }
}
