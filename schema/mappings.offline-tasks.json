                                    "authorization": {
                                        "type": "string"
                                    },
                                    "content-length": {
                                        "type": "string"
                                    },
                                    "content-type": {
                                        "type": "string"
                                    },
                                    "host": {
                                        "type": "string"
                                    },
                                    "user-agent": {
                                        "type": "string"
                                    }
                                }
                            },
                            "saved": {
                                "type": "string"
                            },
                            "url": {
                                "type": "string"
                            }
                        }
                    },
                    "task": {
                        "type": "string"
                    },
                    "task_id": {
                        "type": "string",
	                "index" : "not_analyzed"
                    },
                    "event_id": {
                        "type": "string",
	                "index" : "not_analyzed"
                    }
                }
            },
            "schedule": {
                "properties": {
                    "@event": {
                        "type": "string"
                    },
                    "@timestamp": {
                        "format": "dateOptionalTime",
                        "type": "date"
                    },
                    "@version": {
                        "type": "string"
                    },
                    "id": {
                        "type": "string"
                    },
                    "rsp": {
                        "properties": {
                            "ok": {
                                "type": "string"
                            }
                        }
                    },
                    "task": {
                        "type": "string"
                    },
                    "type": {
                        "type": "string"
                    }
                }
            }
        }
    }
}
