{
    "settings":{
	"analysis":{
	    "analyzer":{
		"autocomplete":{
		    "type":"custom",
		    "tokenizer":"standard",
		    "filter":[ "standard", "lowercase", "stop", "kstem", "ngram" ] 
		}
	    },
	    "filter":{
		"ngram":{
		    "type":"ngram",
		    "min_gram":2,
		    "max_gram":15
		}
	    }
	}
    },
    "mappings": {
	"autocomplete": {
	    "properties": {
		"names": {
		    "type": "string",
		    "analyzer": "autocomplete_analyzer",
		    "boost": 10
		}
	    }
	}
    }
}
